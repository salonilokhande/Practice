public class Main
{
	public static void main(String[] args) {
		int arr[]={
		    0,1,1,0,0,1,0,1
		};
		int ZeroCount=0;
		for(int ele : arr)
		{
		    if(ele == 0)
		    ZeroCount++;
		}
		
		for(int i=0;i<ZeroCount;i++)
		{
		    arr[i]=0;
		}
		for(int i=ZeroCount;i<arr.length;i++)
		{
		    arr[i]=1;
		}
		
	for(int sec : arr)
	{
	    System.out.print(sec + " ");
	}
	}
}
