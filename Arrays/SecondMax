public class Main
{
	public static void main(String[] args) {
		int arr[]={
		    2,5,1,4,8,6,9
		};
		int max=arr[0];
		int secMax=arr[0];
		for(int i=0;i<arr.length;i++)
		{
		    if(arr[i]>max)
		    {
		        max=arr[i];
		    }
		}
		System.out.println(max);
		
		for(int i=0;i<arr.length;i++)
		{
		    if(arr[i]>secMax && arr[i] != max)
		    {
		        secMax=arr[i];
		    }
		}
		
		System.out.println(secMax);
	}
}
