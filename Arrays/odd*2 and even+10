import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int arr[]= {  1,2,3,4,5,6	};
		int min=arr[0];
		
		for(int i=0;i<arr.length;i++)
		{
		    if(i%2 ==0)
		    {
		        arr[i]=arr[i]+10;
		    }
		    else
		    {
		        arr[i]=arr[i]*2;
		    }
		}
	
	for(int ele : arr)
	{
	    System.out.print(ele+ " , ");
	}
	}
}
