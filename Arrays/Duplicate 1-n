//Getting duplicates when the array is given from 1 to n

public class Main
{
	public static void main(String[] args) {
		int arr[]={
		    1,2,3,4,5,4,6
		};
		int n=arr.length-1;
	    int sum=0;
	    int actualSum=n*(n+1)/2;
	    for(int i=0;i<=n;i++)
	    {
	        sum=sum+arr[i];
	    }
	    
	    System.out.print(sum-actualSum);
	    
	}
}
